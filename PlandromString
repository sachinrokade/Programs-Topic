 * @author Shidhhi
 */
class Palidrom 
{
    public static void main(String arg[])
    {
        String  in="niTin";
        
       // char toCharArray[] = in.toCharArray();q
        int j=1;
        int count=0;
        for(int i=0;i<(in.length()/2);i++)
        {
           
            if(in.indexOf(i)==in.indexOf(i+1))
                count=0;
            else
                count=count+1;
        }
        if(count==0)
            System.out.print("Yes");
        else
            System.out.print("No");
    }
}
